<!--■■■■■■■■■■■■■■■■■■ GERZEE ■■■■■■■■■■■■■■■■■■■■■■■-->
<AlienRace.BackstoryDef>
    <defName>ZRPF_BACKSTORY_BIOPRINTED_GERZEE_HYBRID_DEF</defName>
    <title>bio-printed: GZ hybrid</title>
    <titleShort>gerzee hybrid</titleShort>
    <baseDescription>[PAWN_nameDef] was bio-printed. </baseDescription>
    
    <slot>Childhood</slot>
    
    
    <disallowedTraits>
      <li><defName>ShootingAccuracy</defName></li>
    </disallowedTraits>
    
    <skillGains>
      <li>
        <defName>Artistic</defName>
        <amount>-20</amount>
      </li>
      <li>
        <defName>Social</defName>
        <amount>-20</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>-5</amount>
      </li>
    </skillGains> 
    
    <spawnCategories>
      <li>Zeta_Reticulan_Backstories_Childhood_Gerzee</li>
    </spawnCategories>
</AlienRace.BackstoryDef>

